{
  "name": "eslint-config-airbnb-angular",
  "version": "1.0.0",
  "description": "Airbnb's ESLint config with Angular and TypeScript support",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --cache .",
    "test": "npm run lint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/efoken/eslint-config-airbnb-angular.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "javascript",
    "typescript",
    "airbnb",
    "config",
    "styleguide"
  ],
  "author": "Eike Foken <eikefoken@gmail.com> (https://eikefoken.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/efoken/eslint-config-airbnb-angular/issues"
  },
  "homepage": "https://github.com/efoken/eslint-config-airbnb-angular",
  "dependencies": {
    "@typescript-eslint/parser": "^2.0.0",
    "eslint-config-airbnb-base": "^14.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">= 8"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "eslint": "^5.16.0 || ^6.2.2",
    "eslint-plugin-import": "^2.18.2"
  }
}
